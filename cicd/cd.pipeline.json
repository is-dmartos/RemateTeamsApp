                            "taskId": "d9bafed4-0b18-4f58-968d-86655b4d2ce9",
                            "version": "2.*",
                            "name": "Add SPFx App",
                            "refName": "",
                            "enabled": true,
                            "alwaysRun": false,
                            "continueOnError": false,
                            "timeoutInMinutes": 0,
                            "definitionType": "task",
                            "overrideInputs": {},
                            "condition": "succeeded()",
                            "inputs": {
                                "script": "Write-Host \"$(System.DefaultWorkingDirectory)/_Remate/drop/remate-teams-app.sppkg\"\nm365 spo app add -p $(System.DefaultWorkingDirectory)/_Remate/drop/remate-teams-app.sppkg --overwrite",
                                "workingDirectory": "",
                                "failOnStderr": "false"
                            }
                        },
