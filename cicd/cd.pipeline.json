                                "failOnStderr": "false"
                            }
                        },
                        {
                            "environment": {},
                            "taskId": "e213ff0f-5d5c-4791-802d-52ea3e7be1f1",
                            "version": "2.*",
                            "name": "Deploy Teams App",
                            "refName": "",
                            "enabled": true,
                            "alwaysRun": false,
                            "continueOnError": false,
                            "timeoutInMinutes": 0,
                            "definitionType": "task",
                            "overrideInputs": {},
                            "condition": "succeeded()",
                            "inputs": {
                                "targetType": "inline",
                                "filePath": "",
                                "arguments": "",
                                "script": "m365 login --authType password --userName $(username) --password $(password)\n \n#try to get app with manifest id\n$catalogApps = m365 teams app list -o json | ConvertFrom-Json\n$existingApp = $catalogApps | Where-Object {$_.externalId -eq \"c057e8bc-2da2-4fd5-9d87-a0282874bed4\" }\n \nif($existingApp){\n    #app already exists so update it.\n    #id used to update is the one returned from the teams app list call\nWrite-Host \"The app exists. Updating...\"\n    m365 teams app update --id $existingApp.id --filePath \"$(System.DefaultWorkingDirectory)/_Remate/drop/TeamsSPFxApp.zip\"\n}\nelse{\n    #app does not exist so add it.\nWrite-Host \"App not found. Installing $(System.DefaultWorkingDirectory)/_Remate/drop/TeamsSPFxApp.zip\"\n    m365 teams app publish --filePath \"$(System.DefaultWorkingDirectory)/_Remate/drop/TeamsSPFxApp.zip\" --verbose\n}",
