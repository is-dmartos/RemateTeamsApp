                                "failOnStderr": "false",
                                "showWarnings": "false",
                                "ignoreLASTEXITCODE": "false",
                                "pwsh": "false",
                                "workingDirectory": "$(System.DefaultWorkingDirectory)/_Remate",
                                "runScriptInSeparateScope": "false"
                            }
                        },
                        {
                            "environment": {},
                            "taskId": "e213ff0f-5d5c-4791-802d-52ea3e7be1f1",
                            "version": "2.*",
                            "name": "Approve pending API requests",
                            "refName": "",
                            "enabled": true,
                            "alwaysRun": false,
                            "continueOnError": false,
                            "timeoutInMinutes": 0,
                            "definitionType": "task",
                            "overrideInputs": {},
                            "condition": "succeeded()",
                            "inputs": {
                                "targetType": "inline",
                                "filePath": "",
                                "arguments": "",
                                "script": "m365 login --authType password --userName $(username) --password $(password)\n \n$requests = m365 spo serviceprincipal permissionrequest list -o json | ConvertFrom-Json\n\nforeach ($request in $requests)\n{\n   m365 spo serviceprincipal permissionrequest approve --requestId $request.Id\n}",
                                "errorActionPreference": "stop",
